List = ({people}) => {\n\n    const theme = useContext(ThemeContext)\n\n\n    return (\n        <Wrapper>\n            {people.map(person =>\n                <PeopleListItem person={person} key={person.name}/>\n            )}\n        </Wrapper>\n    );\n}\n\n\n\n","/Users/jackilrajnicant/WebstormProjects/learning-react/src/PeopleListItem.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/CounterButton.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/CongratulationsMessage.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/index.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/PeopleListPage.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/HomePage.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/CounterButtonPage.js",["119","120","121"],"import React, {useState, useEffect} from 'react';\nimport {useParams, useLocation} from 'react-router-dom';\nimport {parse} from 'query-string';\nimport {CounterButton} from \"../CounterButton\";\nimport {CongratulationsMessage} from \"../CongratulationsMessage\";\nimport {DisplayIf} from \"../DisplayIf\";\nimport {usePersistentState} from \"../usePersistentState\";\n\nexport const CounterButtonPage = () => {\n    // const location = useLocation()\n    // let startingValue = Number(parse(location.search).startingValue) || 0;\n    // const {name} = useParams();\n    // const [numberOfClicks, setNumberOfClicks] = useState(Number(localStorage.getItem('numberOfClicks')) || 0);\n    const [hideMessage, setHideMessage] = useState(localStorage.getItem('hideMessage')==='true' || false);\n\n    const [numberOfClicks, setNumberOfClicks] = usePersistentState('numberOfClicks',0, Number)\n    //const [hideMessage, setHideMessage] = usePersistentState('hideMessage',false, String)\n    const increment = () => setNumberOfClicks(numberOfClicks + 1);\n\n    useEffect(()=> {\n        localStorage.setItem('numberOfClicks',numberOfClicks.toString());\n        localStorage.setItem('hideMessage', hideMessage.toString())\n    }, [numberOfClicks, hideMessage])\n\n    return (\n        <>\n            <h1>Counter Button Page</h1>\n            {/*{hideMessage ? null : <CongratulationsMessage*/}\n            {/*    numberOfClicks={numberOfClicks}*/}\n            {/*    threshold={10}*/}\n            {/*    onHide={() =>  setHideMessage(true)}/>}*/}\n\n            <DisplayIf condition={!hideMessage && numberOfClicks >= 10}>\n                <CongratulationsMessage\n                    threshold={10}\n                    onHide={() => setHideMessage(true)}/>\n            </DisplayIf>\n            <CounterButton onIncrement={increment} numberOfClicks={numberOfClicks}/>\n        </>\n    )\n}","/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/NotFoundPage.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/ProtectedPage.js",["122"],"import React, {useEffect} from 'react'\nimport {Redirect , useHistory} from 'react-router-dom'\n\nexport const ProtectedPage = () => {\n    // const isAuthed = false;\n    // return isAuthed ? (\n    //     <>\n    //         <h1>Only Authed Users should be able to see this!</h1>\n    //     </>\n    // ) : (\n    //     <Redirect to=\"/\"/>\n    // );\n\n    const history = useHistory();\n\n    const isAuthed = false ;\n\n    useEffect(() => {\n        if (isAuthed) {\n            history.push('/')\n        }\n    })\n\n    return (\n        <h1>Only Authed Users should be able to see this!</h1>\n    );\n\n}","/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/ControlledFormPage.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/UncontrolledFormPage.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/NavBar.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/FormsNavBar.js",[],"/Users/jackilrajnicant/WebstormProjects/learning-react/src/pages/UserProfilePage.js",["123"